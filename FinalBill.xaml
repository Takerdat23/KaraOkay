<Window x:Class="Wpf_Karaokay.FinalBill"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_Karaokay"
        mc:Ignorable="d"
        Title="Final Bill" Height="760" Width="450"
        DataContext="{DynamicResource FinalVM}">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,37,0,0" TextWrapping="Wrap" Text="Receipt" VerticalAlignment="Top" Height="44" Width="132" FontFamily="Gill Sans MT" FontSize="30" TextAlignment="Center"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,86,0,0" TextWrapping="Wrap" Text="{Binding FinalReceipt}" VerticalAlignment="Top" Height="648" Width="430" FontSize="20"/>

    </Grid>
</Window>
